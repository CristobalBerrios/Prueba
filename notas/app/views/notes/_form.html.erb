<%= form_for(@note) do |f| %>
	<div class="row">
		<div class="input-field col s12">
			<%= f.text_area :texto,placeholder:"Escribe la nota",class:"materialize-textarea" %>
		</div>
	</div>
	<div class="row">
		<div class="col s12">
			<%= f.submit "Guardar nota", class:"btn waves-effect waves-light" %>
		</div>
	</div>
<%end%>